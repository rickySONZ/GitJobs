<h1>Create Account</h1>
<br>
<%= render partial: 'errors/errors', locals: {model: @user} if @user %>
<%= form_for :user, url: '/users' do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br><br>
    <%= f.label :email %>
    <%= f.text_field :email %><br><br>
    <%= f.label :password %>
    <%= f.text_field :password %><br><br>
    Password Confirmation <%= f.text_field :password_confirmation %><br><br>    <%= f.submit "Submit" %>
  <% end %> 

  <br>
  <% if !logged_in? %>
    <%= button_to "Log In With Google", "/auth/google_oauth2" %>
    <% end %>
